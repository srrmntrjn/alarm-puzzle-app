{
  "expo": {
    "name": "alarm-puzzle-app",
    "slug": "alarm-puzzle-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alarmpuzzleapp",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "package": "com.alarmpuzzleapp",
      "permissions": [
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "POST_NOTIFICATIONS"
      ]
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#007AFF",
      "androidMode": "default",
      "androidCollapsedTitle": "Alarm"
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#007AFF",
          "sounds": []
        }
      ]
    ],
    "scheme": "alarmapp"
  }
}
